extends default
block css
  link(rel="stylesheet", href="css/sedemat.css")
block content
  div#fecha
    p 
      b FECHA:
      span &nbsp; #{moment(fecha).format('DD/MM/YYYY')}
    p 
      b Codigo de Tramite:
      span &nbsp; #{codigo}
  div.titleWithQR
    p#title
      b #{titulo}
    .QR
      img#QR(src=QR, alt='QR')
  div#planilla
    div(style={'margin-bottom':'25px'})
      div.table(style={'width':'90%', 'font-size':'16px', 'font-weight': 'bold'})
        div.tr
          div(style={'width':'15%'}).td.border-right.border-bottom.center
            p 
              b Nro. Solicitud
          div(style={'width':'20%'}).td.border-right.border-bottom.center
            p
              b Nro. Planilla
          div(style={'width':'15%'}).td.border-right.border-bottom.center
            p
              b MOTIVO
          div(style={'width':'20%'}).td.border-right.border-bottom.center
            p
              b Liquidador
          div(style={'width':'15%'}).td.border-bottom.center.border-right
            p
              b Cajero
          div(style={'width':'15%'}).td.border-bottom.center
            p
              b Nº Factura
        div.tr
          div(style={'width':'15%'}).td.border-right.center
            p #{datos.nroSolicitud}
          div(style={'width':'20%'}).td.border-right.center
            p #{datos.nroPlanilla}
          div(style={'width':'15%'}).td.border-right.center
            p #{datos.motivo}
          div(style={'width':'20%'}).td.border-right.center
            p  #{datos.usuario}
          div(style={'width':'15%'}).td.border-right.center
            p
          div(style={'width':'15%'}).td.center
            p  #{datos.nroFactura}
    div.table(style={'font-size':'16px'})
      div.tr
        div.td(style={'width':'80%'}).border-right.border-bottom
          p
            b Tipo De Trámite:
            span &nbsp; #{datos.tipoTramite}
        div.td(style={'width':'20%'}).border-bottom
          p
            b Cuenta/Contrato.:
            span &nbsp; #{datos.cuentaOContrato}
      div.tr
        div(style={'width':'30%'}).td.border-right
          p
            b Tipo Inmueble:
            span &nbsp; #{datos.tipoInmueble}
        div(style={'width':'10%'}).td.border-right.center
          p
            b FECHA CREACIÓN:
        div(style={'width':'10%'}).td.border-right
          p #{datos.fechaCre}
        div(style={'width':'15%'}).td.border-right.center
          p
            b FECHA LIQUIDACIÓN:          
        div(style={'width':'10%'}).td.border-right
          p #{datos.fechaLiq}
        div(style={'width':'15%'}).td.border-right.center
          p
            b FECHA VENCIMIENTO:         
        div(style={'width':'10%'}).td
          p #{datos.fechaVenc}
    div.table
      div.tr
        div(style={'width':'100%'}).th.border-bottom.center
          p
            b PROPIETARIO DEL INMUEBLE Y SOLICITANTE   
      div.tr
        div(style={'width':'20%'}).td.border-right.border-bottom.center
          p Razón Social:
        div(style={'width':'55%'}).td.border-right.border-bottom
          p #{datos.propietario.razonSocial}
        div(style={'width':'25%'}).td.border-bottom
          p 
            b RIF.:
            span &nbsp; #{datos.propietario.rif}
      div.tr
        div(style={'width':'20%'}).td.border-right.border-bottom.center
          p Denom. Comercial:
        div(style={'width':'80%'}).td.border-bottom
          p 
            span &nbsp; #{datos.propietario.denomComercial}
      div.tr
        div(style={'width':'20%'}).td.border-right.center
          p Dirección:
        div(style={'width':'80%'}).td
          p 
            span &nbsp; #{datos.propietario.direccion}
    div(style={'text-align': '-webkit-right'})  
      div.table
        div.tr
          div(style={'width':'55%'}).th.border-bottom.center.border-right
            p
              b DIRECCIÓN
          div(style={'width':'25%'}).th.border-bottom.center.border-right
            p
              b PERIODOS
          div(style={'width':'20%'}).th.border-bottom.center
            p
              b IMPUESTO 
        div.tr
          div(style={'width':'55%'}).td.border-right
            p 
              span &nbsp; #{datos.direccion}
          div(style={'width':'25%'}).td.border-right
            p 
              span &nbsp; #{datos.periodos}
          div(style={'width':'20%'}).td
            p 
              span &nbsp; #{datos.impuesto}
            
      div(style={'width':'45%','margin-top':0, 'border':'none'}).table
        div.tr
          div(style={'width':'55.4%'}).td.border-right.end
            p
              b TOTAL IVA (16%)
          div(style={'width':'44.6%'}).td.border-bottom.border-right
            p #{datos.totalIva}
        div.tr
          div(style={'width':'55.4%'}).td.border-right.end
            p
              b TOTAL RETENCION IVA (75%):
          div(style={'width':'44.6%'}).td.border-bottom.border-right
            p #{datos.totalRetencionIva}
        div.tr
          div(style={'width':'55.4%'}).td.border-right.end
            p
              b TOTAL IVA A PAGAR:
          div(style={'width':'44.6%'}).td.border-bottom.border-right
            p #{datos.totalIvaPagar}
        div.tr
          div(style={'width':'55.4%'}).td.border-right.end
            p
              b TOTAL IMPUESTO A PAGAR:
          div(style={'width':'44.6%'}).td.border-right
            p #{datos.montoTotalImpuesto}
        div.tr
          div(style={'width':'40%'}).td.end
            p
              b Intereses Moratorios:
          div(style={'width':'60%'}).td.all-border 
            p #{dato.interesMoratorio}
    div.table
      div.tr
        div(style={'width':'20%'}).th.border-bottom.border-right
          p
            b Estatus:
        div(style={'width':'80%'}).th.border-bottom
          p #{datos.estatus}
      div.tr
        div(style={'width':'100%'}).th.border-bottom
          p
            b OBSERVACIONES
      div.tr
        div(style={'width':'100%'}).td
          p
            span &nbsp; #{datos.observacion}
    div.table
      div(style={'background-color':'rgba(0,0,0,0.2)'}).tr
        div(style={'width':'33%'}).td.border-right.center
          p
            b Total Liquidado:
            span &nbsp; #{datos.totalLiq}
        div(style={'width':'33%'}).td.border-right.center
          p
            b Total Recaudado:
            span &nbsp; #{datos.totalRecaudado}
        div(style={'width':'33%'}).td.center
          p
            b Total Credito Fiscal:
            span &nbsp; #{datos.totalCred}

      