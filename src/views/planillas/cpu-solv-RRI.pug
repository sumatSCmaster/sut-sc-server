extends default

block css
  link(rel="stylesheet", href="css/cpu-solv-RRI.css")

block content
  div.row
     p
        b EXP.:
        |  #{codigo}
     p
        b FECHA:
        |  #{moment(fecha).format('DD/MM/YYYY')}
  div 
     h1 REGISTRO REFERENCIAL DEL INMUEBLE 
  div.container-data 
     if datos.usuario.ubicadoEn
         p DIRECCIÓN: #{datos.usuario.ubicadoEn}
     if datos.usuario.parroquiaEdificio
         p PARROQUIA: #{datos.usuario.parroquiaEdificio}
     if datos.ubicadoEn
         p DIRECCIÓN: #{datos.ubicadoEn}
     if datos.parroquiaEdificio
         p PARROQUIA: #{datos.parroquiaEdificio}
     P AREA DE TERRENO: #{datos.areaTerreno ? datos.areaTerreno + ' M2' : 'N/A'}
     P AREA DE CONSTRUCCIÓN: #{datos.areaConstruccion ? datos.areaConstruccion + ' M2' : 'N/A'}
     p R.R INMUEBLE: 
        h1 #{datos.codigoRRI}
  div 
     h1#watermark VALIDO USO CATASTRO
